{
  "name": "geolocation-file",
  "version": "1.0.0",
  "description": "geolocation file upload",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "build": "npm-run-all clean lint tsc",
    "dev:start": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts --exec npm run dev:start",
    "start": "node --max-old-space-size=4096 ."
  },
  "keywords": [
    "node.js",
    "express",
    "mongodb"
  ],
  "author": "Freddy Lemus",
  "license": "MIT",
  "dependencies": {
    "@types/express-fileupload": "^1.1.5",
    "connect-multiparty": "^2.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "fast-csv": "^4.3.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/fs-extra": "9.0.4",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.14.10",
    "@types/shelljs": "0.8.8",
    "fs-extra": "9.0.1",
    "nodemon": "2.0.6",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "ts-node": "9.0.0",
    "tslint": "6.1.3",
    "typescript": "4.1.2"
  }
}
